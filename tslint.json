{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "defaultSeverity": "error",
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "no-console": false,
    "object-literal-sort-keys": false
  }
}
